IDR_MAINFRAME MENU
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建(&N)\tCtrl+N",              ID_FILE_NEW
        MENUITEM "打开(&O)...\tCtrl+O",           ID_FILE_OPEN
        MENUITEM "保存(&S)\tCtrl+S",              ID_FILE_SAVE
        MENUITEM "另存为(&A)...",                  ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "打印(&P)...\tCtrl+P",           ID_FILE_PRINT
        MENUITEM "打印预览(&V)",                    ID_FILE_PRINT_PREVIEW
        MENUITEM "打印设置(&R)...",                 ID_FILE_PRINT_SETUP
        MENUITEM SEPARATOR
        MENUITEM "最近的文件",                       ID_FILE_MRU_FILE1
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                      ID_APP_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "撤消(&U)\tCtrl+Z",              ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "剪切(&T)\tCtrl+X",              ID_EDIT_CUT
        MENUITEM "复制(&C)\tCtrl+C",              ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+V",              ID_EDIT_PASTE
    END
    POPUP "视图(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                     ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                     ID_VIEW_STATUS_BAR
    END
    POPUP "绘画(&D)"
    BEGIN
        MENUITEM "直线(&L)",                      ID_MENU_DRAW_LINE
        MENUITEM "矩形(&R)",                      ID_MENU_DRAW_RECT
        MENUITEM "椭圆(&E)",                      ID_MENU_DRAW_ELLI
        MENUITEM "铅笔(&P)",                      ID_LINE_PEN
        MENUITEM "清屏(&C)",                      ID_MENU_DRAW_CLEAR
    END
    POPUP "线条(&L)"
    BEGIN
        MENUITEM "颜色(&C)",                      ID_MENU_LINE_COLOR
        POPUP "样式(&S)"
        BEGIN
            MENUITEM "实心(&0)",                      ID_MENU_LINE_SOLID
            MENUITEM "虚线(&1)",                      ID_MENU_LINE_DASH
            MENUITEM "点线(&2)",                      ID_MENU_LINE_DOT
            MENUITEM "虚点线(&3)",                     ID_MENU_LINE_DASHDOT
            MENUITEM "虚点点线(&4)",                    ID_MENU_LINE_DASHDOTDOT
            MENUITEM "空笔(&5)",                      ID_MENU_LINE_NULL
        END
        MENUITEM "宽度(&W)",                      ID_MENU_LINE_WIDTH
    END
    POPUP "笔刷(&B)"
    BEGIN
        MENUITEM "颜色(&C)",                      ID_MENU_BRUSH_COLOR
        MENUITEM "透明(&T)",                      ID_MENU_BRUSH_TRANSPARENT
        POPUP "样式(&S)"
        BEGIN
            MENUITEM "水平线(&0)",                     ID_MENU_BRUSH_HORIZONTAL
            MENUITEM "垂直线(&1)",                     ID_MENU_BRUSH_VERTICAL
            MENUITEM "正斜线(&2)",                     ID_MENU_BRUSH_FDIAGONAL
            MENUITEM "反斜线(&3)",                     ID_MENU_BRUSH_BDIAGONAL
            MENUITEM "十字线(&4)",                     ID_MENU_BRUSH_CROSS
            MENUITEM "斜十字线(&5)",                    ID_MENU_BRUSH_DIAGCROSS
            MENUITEM "填充(&6)",                      ID_MENU_BRUSH_FILL
        END
    END
    POPUP "网络(&N)"
    BEGIN
        MENUITEM "打开服务端(&S)",                   ID_MENU_NET_SERVER
        MENUITEM "打开客户端(&C)",                   ID_MENU_NET_CLIENT
    END
    POPUP "小功能(&F)"
    BEGIN
        MENUITEM "BGM(&P)",                     ID_BGM_PLAY
        MENUITEM "图画保存(&C)",                    ID_CLIENT_SAVE
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 MFCDraw(&A)...",           ID_APP_ABOUT
    END
END


